    "@types/node": "^18.0.0",
    "fastify": "^4.3.0",
    "nock": "^13.2.1",
    "simple-get": "^4.0.0",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0",
    "tap": "^16.0.0",
